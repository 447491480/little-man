{
  "restartable": "rs",
  "ignore": [
    ".git",
    "sessions",
    "logs",
    "node_modules/**/node_modules"
  ],
  "verbose": true,
  "execMap": {
    "js": "node --harmony"
  },
  "events": {
    "restart": ""
  },
  "watch": [
    "*.js",
    "*.html",
    "*.css",
    "*.json",
    "*.yaml"
  ],
  "env": {
  },
  "ext": "js json"
}